a:22:{s:7:"attribs";a:6:{s:5:"xmlns";s:35:"http://pear.php.net/dtd/package-2.0";s:11:"xmlns:tasks";s:33:"http://pear.php.net/dtd/tasks-1.0";s:9:"xmlns:xsi";s:41:"http://www.w3.org/2001/XMLSchema-instance";s:15:"packagerversion";s:5:"1.9.1";s:7:"version";s:3:"2.0";s:18:"xsi:schemaLocation";s:147:"http://pear.php.net/dtd/tasks-1.0 http://pear.php.net/dtd/tasks-1.0.xsd http://pear.php.net/dtd/package-2.0 http://pear.php.net/dtd/package-2.0.xsd";}s:4:"name";s:37:"sfTextDateInputJQueryDatePickerPlugin";s:7:"channel";s:27:"plugins.symfony-project.org";s:7:"summary";s:156:"Allows date or date/time input via a simple text field, with automatic date or date/time re-formatting and a DatePicker pop-up calendar for selecting dates.";s:11:"description";s:892:"For high-speed data entry, there's nothing like being able to enter a date or date/time in any of the normally accepted formats and have it automatically re-format for you.  Datejs is the key to enabling this functionality.  We've also thrown in the jQuery DatePicker component for good measure, which allows you to browse a calendar to select a date.  This is activated by a button to the right of the date input field.

All of the date and date/time widgets which come with Symfony are clumsy, difficult to use, and not suited for high-speed data entry.

This widget plugin will allow you to use a normal text input field for date or date/time entry, and have a button next to the input which pops up a jQuery DatePicker component for selecting the date by hand.  It uses Datejs to auto-reformat any manually-entered date or date/time into standard YYYY-MM-DD or YYYY-MM-DD hh:mm:ss format.";s:4:"lead";a:4:{s:4:"name";s:15:"Ronald B. Cemer";s:4:"user";s:8:"roncemer";s:5:"email";s:16:"ron@roncemer.com";s:6:"active";s:3:"yes";}s:4:"date";s:10:"2011-07-06";s:4:"time";s:8:"04:25:47";s:7:"version";a:2:{s:7:"release";s:5:"1.0.1";s:3:"api";s:5:"1.0.0";}s:9:"stability";a:2:{s:7:"release";s:6:"stable";s:3:"api";s:6:"stable";}s:7:"license";a:2:{s:7:"attribs";a:1:{s:3:"uri";s:38:"http://www.symfony-project.org/license";}s:8:"_content";s:11:"MIT license";}s:5:"notes";s:1:"-";s:8:"contents";a:1:{s:3:"dir";a:2:{s:7:"attribs";a:1:{s:4:"name";s:1:"/";}s:4:"file";a:5:{i:0;a:1:{s:7:"attribs";a:3:{s:6:"md5sum";s:32:"b9e52020104634a14f185c6b5ebe2d49";s:4:"name";s:67:"config/sfTextDateInputJQueryDatePickerPluginConfiguration.class.php";s:4:"role";s:4:"data";}}i:1;a:1:{s:7:"attribs";a:3:{s:6:"md5sum";s:32:"5bbb3f6ec9a6542f62845ac927eb4278";s:4:"name";s:67:"lib/widget/.sfWidgetFormTextDateInputJQueryDatePicker.class.php.swp";s:4:"role";s:4:"data";}}i:2;a:1:{s:7:"attribs";a:3:{s:6:"md5sum";s:32:"bac9f6bc0311517035553ef94628975d";s:4:"name";s:62:"lib/widget/sfWidgetFormTextDateInputJQueryDatePicker.class.php";s:4:"role";s:4:"data";}}i:3;a:1:{s:7:"attribs";a:3:{s:6:"md5sum";s:32:"37b2bde029c3be652309ec161a3c32f8";s:4:"name";s:7:"LICENSE";s:4:"role";s:4:"data";}}i:4;a:1:{s:7:"attribs";a:3:{s:6:"md5sum";s:32:"86b62d0a7ef96c971ec6db80e98ff1df";s:4:"name";s:6:"README";s:4:"role";s:4:"data";}}}}}s:12:"dependencies";a:1:{s:8:"required";a:2:{s:3:"php";a:1:{s:3:"min";s:5:"5.2.4";}s:13:"pearinstaller";a:1:{s:3:"min";s:5:"1.4.1";}}}s:10:"phprelease";s:0:"";s:9:"changelog";a:1:{s:7:"release";a:2:{i:0;a:5:{s:7:"version";a:2:{s:7:"release";s:5:"1.0.1";s:3:"api"